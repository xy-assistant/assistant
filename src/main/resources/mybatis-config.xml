<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <settings>
        <!--开启缓存 -->
        <setting name="cacheEnabled" value="false" />
    </settings>

    <!--别名映射 -->
    <typeAliases>
        <package name="com.hpin.assistant.domain" />
    </typeAliases>

    <!-- mapper扫描 -->
    <mappers>
        <package name="org.i5j.xydms.dao.mapper"/>
    </mappers>
</configuration>