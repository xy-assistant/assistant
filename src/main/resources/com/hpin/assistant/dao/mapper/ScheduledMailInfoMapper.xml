<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hpin.assistant.dao.mapper.ScheduledMailInfoMapper">
    <insert id="insertMainInfo" parameterType="MailInfo">
        INSERT INTO QRTZ_TASK_MAIL_INFO
            (id,subject,`from`,message,copyList,sendList,nickName,createDate,modifyDate)
        VALUES
            (#{id}, #{subject}, #{from},#{message},#{copyList},#{sendList},#{nickName},#{createDate},#{modifyDate})
    </insert>
</mapper>